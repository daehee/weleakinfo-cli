{
  "name": "weleakinfo-cli",
  "version": "0.1.0",
  "description": "CLI interface for WeLeakInfo API",
  "author": {
    "name": "Daehee Park",
    "email": "hello@daehee.com",
    "url": "https://github.com/daehee"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "keywords": [
    "We Leak Info",
    "OSINT",
    "infosec",
    "passwords",
    "breaches"
  ],
  "devDependencies": {
    "eslint": "^6.0.1",
    "prettier": "^1.18.2",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-config-xo": "^0.26.0",
    "jest": "^24.8.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "https://github.com/daehee/weleakinfo-cli",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "dotenv": "^8.1.0",
    "inquirer": "^6.5.1"
  }
}
